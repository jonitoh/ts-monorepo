	"main": "build/index.js",
	"typings": "build/index.d.ts",
	"scripts": {
		"format": "prettier --config ../../.prettierrc --ignore-path ../../.prettierignore --write .",
		"lint": "eslint --ignore-path ../../.eslintignore --fix .",
		"verify": "npm-run-all --sequential format lint",
		"dev": "webpack serve --node-env development --config ./config/webpack.config.ts --env configPath=../../.env addon=$npm_config_addon",
		"clean": "rimraf build ../../coverage/client ../../.jest_cache/client",
		"purge": "npm run clean && rimraf node_modules",
		"compile": "webpack build --node-env production --config ./config/webpack.config.ts --env configPath=../../.env addon=$npm_config_addon",
		"build": "rimraf build && npm run compile",
		"start": "serve build",
		"start:dev": "serve build --debug",
		"prepublishOnly": "npm run build",
		"watch": "webpack watch --node-env development --config ./config/webpack.config.ts --progress --env configPath=../../.env addon=$npm_config_addon",
		"TODOtest": "jest --detectOpenHandles --colors",
		"TODOtest:watch": "jest --detectOpenHandles --colors --coverage --watchAll",
		"TODOtest:coverage": "jest --ci --silent --colors --coverage",
		"TODOtest:view-coverage": "serve coverage/lcov-report"
	},
