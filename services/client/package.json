	"bugs": {
		"url": "https://github.com/jonitoh/ts-monorepo/issues"
	},
	"repository": {
		"type": "git",
		"directory": "services/client",
		"url": "git+https://github.com/jonitoh/ts-monorepo.git"
	},
	"homepage": "https://github.com/jonitoh/ts-monorepo#readme",
	"engines": {
		"node": "17.0.1",
		"npm": "8.1.0"
	},
	"main": "build/index.js",
	"typings": "build/index.d.ts",
	"scripts": {
		"format": "prettier --config ../../.prettierrc --ignore-path ../../.prettierignore '{src,config}/*.{ts,tsx}'",
		"postformat": "rimraf node_modules",
		"format:fix": "prettier --config ../../.prettierrc --ignore-path ../../.prettierignore --write '{src,config}/*.{ts,tsx}'",
		"postformat:fix": "rimraf node_modules",
		"lint": "eslint --ignore-path ../../.eslintignore --ext .ts,.tsx,.js,.jsx",
		"lint:fix": "eslint --ignore-path ../../.eslintignore --ext .ts,.tsx,.js,.jsx --fix",
		"verify": "npm-run-all --sequential 'format:fix' 'lint:fix'",
		"test": "echo 'Error: no test specified'",
		"dev": "webpack serve --node-env development --config ./config/webpack.config.ts --env configPath=../../.env addon=$npm_config_addon",
		"clean": "rimraf build",
		"compile": "webpack build --node-env production --config ./config/webpack.config.ts --env configPath=../../.env addon=$npm_config_addon",
		"build": "npm-run-all --sequential clean compile",
		"start": "serve build",
		"start:dev": "serve build --debug",
		"prepublishOnly": "npm run build",
		"watch": "webpack watch --node-env development --config ./config/webpack.config.ts --progress --env configPath=../../.env addon=$npm_config_addon"
	}
}
