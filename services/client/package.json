    "compile": "tsc -b -p tsconfig.build.json",
    "postcompile": "cp './package.json' ./build/",
    "prebuild": "npm run clean && npm run compile",
    "build": "webpack --config ./config/webpack.config.ts --env mode=prod configPath=../../.env addon=$npm_config_addon",
    "prepublishOnly": "npm run build",
    "start": "ts-node -r tsconfig-paths/register src/index.tsx",
    "watch": "tsc -b -p tsconfig.build.json -w --preserveWatchOutput"
